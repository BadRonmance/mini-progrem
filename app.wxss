@import "./assets/css/public.wxss";
@import "./assets/css/iconfont.wxss";
@import "./assets/css/button.wxss";

page {
    font-family: PingFang-SC-Bold;
    width: 100%;
    height: 100%;
    overflow-x: hidden;
    background: #f9f9f9;
    font-size: 32rpx;
    color: #333;
}

view {
    box-sizing: border-box;
}

.btn {
    height: 90rpx;
    line-height: 90rpx;
    width: 100%;
    background: #E3BF68;
    border-radius: 50px;
    font-size: 34rpx;
    font-weight: bold;
}

.default_color {
    color: #808080;
}