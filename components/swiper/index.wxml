<swiper class="swiper" indicator-color="{{system.indicatorColor}}"
      indicator-active-color="{{system.indicatorActiveColor}}" indicator-dots="{{system.indicatorDots}}"
      autoplay="{{system.autoplay}}" interval="{{system.interval}}" duration="{{system.duration}}" style="height:{{height}}">
      <block wx:for="{{swiperData}}" wx:key="*this">
            <swiper-item>
                  <image src="{{item.advertising.content}}" class="wid_hei" mode="aspectFill"></image>
            </swiper-item>
      </block>
</swiper>